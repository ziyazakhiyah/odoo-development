<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="custom_pos.ControlButtons" t-inherit="point_of_sale.ControlButtons" t-inherit-mode="extension">
        <xpath expr="//OrderlineNoteButton" position="after">
             <button class="btn btn-secondary btn-lg py-5" t-on-click="() => this.onClickPopup()">
                 <i class="fa fa-eraser me-1" role="img" title="Clear Ordelines"/>Clear All
             </button>
        </xpath>
    </t>
</templates>
